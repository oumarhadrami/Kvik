<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgrnd"
    tools:context="com.example.abdelkerim.kvik.SplashScreen">

    <ImageView
        android:layout_width="300sp"
        android:layout_height="300sp"

        android:contentDescription="@string/app_name"
        android:src="@drawable/kvik"
        android:id="@+id/imageViewww"
        android:layout_marginLeft="18dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="93dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/kaushalya_vikas_kendra"
        android:textSize="30sp"
        android:textColor="#FF232121"
        android:layout_below="@+id/imageViewww"
        android:layout_alignLeft="@+id/imageViewww"
        android:layout_alignStart="@+id/imageViewww" />
</RelativeLayout>
